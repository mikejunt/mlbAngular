{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "team data",
      "urls": ["https://baseball-api.azurewebsites.net/api/teams","https://baseball-api.azurewebsites.net/api/teams/*"],
      "version": 1,
      "cacheConfig": {
        "maxSize": 31,
        "maxAge": "30d",
        "timeout": "3s",
        "strategy": "performance"
      }
    },{
      "name": "roster data",
      "urls": ["https://lookup-service-prod.mlb.com/json/named.roster_40.bam?*"],
      "version": 1,
      "cacheConfig": {
        "maxSize": 31,
        "maxAge": "30d",
        "timeout": "6s",
        "strategy": "freshness"
      }
    },{
      "name": "team hitting data for splash",
      "urls": ["https://baseball-api.azurewebsites.net/api/hitting?season=2019&pa=300&team=*&position="],
      "version": 1,
      "cacheConfig": {
        "maxSize": 30,
        "maxAge": "30d",
        "timeout": "4s",
        "strategy": "performance"
      } 
    },{
      "name": "team pitching data for splash",
      "urls": ["https://baseball-api.azurewebsites.net/api/hitting?season=2019&team=*&ip=50"],
      "version": 1,
      "cacheConfig": {
        "maxSize": 30,
        "maxAge": "30d",
        "timeout": "4s",
        "strategy": "performance"
      }
    },{
      "name": "transaction data",
      "urls": ["https://lookup-service-prod.mlb.com/json/named.transaction_all.bam?*"],
      "version": 1,
      "cacheConfig": {
        "maxSize": 1,
        "maxAge": "1d",
        "timeout": "6s",
        "strategy": "freshness"
      }
    }
  ]
}
